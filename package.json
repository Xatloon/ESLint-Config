{
  "name": "@xats/eslint-config",
  "type": "module",
  "version": "1.2.1",
  "description": "",
  "author": "Xat <i@xatloon.com> (https://xatloon.com)",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nlx @eslint/config-inspector --config eslint.config.js",
    "prepare": "lefthook install && nr build",
    "release": "bumpp && npm publish",
    "build": "nr type:gen && tsup --format esm,cjs --clean --dts",
    "type:gen": "tsx scripts/typegen.ts",
    "type:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.15.0",
    "@html-eslint/eslint-plugin": "^0.27.0",
    "@html-eslint/parser": "^0.27.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@unocss/eslint-plugin": "^0.63.4",
    "astro-eslint-parser": "^1.0.3",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import-x": "^4.3.1",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-toml": "^0.11.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-typegen": "^0.3.2",
    "globals": "^15.11.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "toml-eslint-parser": "^0.10.0",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@types/node": "^22.7.7",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "lefthook": "^1.7.22",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
